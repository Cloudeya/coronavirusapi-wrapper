{
  "name": "coronavirusapi-wrapper",
  "version": "2.0.0",
  "description": "A wrapper to work with the Coronavirus API.",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc && npm run build:esm && npm run build:umd && npm run build:umd:min",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:umd": "rollup dist/esm/index.js --format umd --name coronavirusapi-wrapper --o dist/umd/coronavirusapi-wrapper.js",
    "build:umd:min": "cd dist/umd && uglifyjs --compress --mangle --comments --output coronavirusapi-wrapper.min.js -- coronavirusapi-wrapper.js",
    "test": "jest --config jest.config.json --coverage && coveralls < coverage/lcov.info",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cloudeya/coronavirusapi-wrapper.git"
  },
  "keywords": [
    "coronavirus",
    "covid19",
    "api",
    "wrapper",
    "js",
    "tdd",
    "library"
  ],
  "author": "Finbarrs Oketunji <tech@cloudeya.org> (https://cloudeya.org/)",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/Cloudeya/coronavirusapi-wrapper/issues"
  },
  "homepage": "https://github.com/Cloudeya/coronavirusapi-wrapper#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.10.2",
    "coveralls": "^3.1.0",
    "jest": "^26.4.2",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "rollup": "^2.27.0",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "uglify-js": "^3.10.4"
  },
  "dependencies": {}
}
